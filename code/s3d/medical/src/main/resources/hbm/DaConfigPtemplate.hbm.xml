<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.s3d.webapps.da.config.persistence.DaConfigPtemplate"
		table="da_config_ptemplate">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdVersion"
			column="fd_version"
			update="true"
			insert="true"
			not-null="true" />
		
		<property
			name="fdCreateTime"
			column="fd_create_time"
			update="true"
			insert="true"
			not-null="true" />
		
		<property
			name="fdLastModifiedTime"
			column="fd_list_modi_time"
			update="true"
			insert="true"
			not-null="false" />
		
		<property
			name="fdHTMLContent"
			column="fd_html_content"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.s3d.webapps.common.dao.ClobStringType"
			lazy="true" />			
			
	</class>
</hibernate-mapping>
