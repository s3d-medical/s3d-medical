<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.s3d.webapps.da.customer.persistence.DaCustomerShouye"
		table="da_customer_shouye">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdCreateTime"
			column="fd_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdLastModifiedTime"
			column="fd_last_modified_time"
			update="true"
			insert="true"
			not-null="true" />

		<property
			name="fdFileNo"
			column="fd_file_no"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<many-to-one
			name="fdLabel"
			column="fd_label_id"
			insert="true"
			update="true"
			not-null="true" />
			
		<many-to-one
			name="fdPicture"
			column="fd_picture_id"
			insert="true"
			update="true"
			not-null="true" />
		
		<many-to-one
			name="fdTemplate"
			column="fd_template_id"
			insert="true"
			update="true"
			not-null="true" />
			
	</class>
</hibernate-mapping>
