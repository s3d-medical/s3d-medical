<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.s3d.webapps.da.customer.persistence.DaCustomerHospital"
		table="da_customer_hospital"
		discriminator-value="not null">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<discriminator
			column="fd_org_type"
			type="int" />
			
		<property
			name="fdOrgType"
			column="fd_org_type"
			update="false"
			insert="false"
			not-null="true" />
			
		<property
			name="fdCode"
			column="fd_code"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdDescription"
			column="fd_description"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
		
		<property
			name="fdAlterTime"
			column="fd_alter_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdHierarchyId"
			column="fd_hierarchy_id"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
			
		<many-to-one
			name="hbmParent"
			column="fd_parent_id"
			insert="true"
			update="true"
			not-null="false" />

	</class>
</hibernate-mapping>
