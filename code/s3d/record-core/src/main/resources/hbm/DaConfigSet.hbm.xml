<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.s3d.webapps.da.config.persistence.DaConfigSet"
		table="da_config_set">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdType"
			column="fd_type"
			update="true"
			insert="true"
			length="36"
			not-null="true" />

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdCode"
			column="fd_code"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdShortcut"
			column="fd_shortcut"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<property
			name="fdExclusive"
			column="fd_exclusive"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdStatus"
			column="fd_status"
			update="true"
			insert="true"
			length="4"
			not-null="true" />

	</class>
</hibernate-mapping>
