<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.s3d.webapps.da.customer.persistence.DaCustomerPicture"
		table="da_customer_picture">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdPicName"
			column="fd_pic_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />
			
		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdPicPath"
			column="fd_pic_path"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdPicType"
			column="fd_pic_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdCreateTime"
			column="fd_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdLastModifiedTime"
			column="fd_last_modified_time"
			update="true"
			insert="true"
			not-null="true" />

		<property
			name="fdMd5Hash"
			column="fd_md5_hash"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdThumb"
			type="blob"
			lazy="true"
			column="fd_thumb"
			length="1000000" />
			
		<property
			name="fdFileNo"
			column="fd_file_no"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<many-to-one
			name="fdLabel"
			column="fd_label_id"
			insert="true"
			update="true"
			not-null="true" />

		<many-to-one
			name="fdCategory"
			column="fd_category_id"
			insert="true"
			update="true"
			not-null="false" />

	</class>
</hibernate-mapping>
