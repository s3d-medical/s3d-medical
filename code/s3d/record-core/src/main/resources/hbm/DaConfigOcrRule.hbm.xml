<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.s3d.webapps.da.config.persistence.DaConfigOcrRule"
		table="da_config_rule">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdOrder"
			column="fd_order"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

		<property
			name="fdExclusive"
			column="fd_exclusive"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdKeyword"
			column="fd_keyword"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdIsRegex"
			column="fd_is_regex"
			update="true"
			insert="true"
			length="1"
			not-null="false" />

		<property
			name="fdStatus"
			column="fd_status"
			update="true"
			insert="true"
			length="4"
			not-null="true" />

		<many-to-one
			name="fdCategory"
			column="fd_category_id"
			insert="true"
			update="true"
			not-null="true" />

	</class>
</hibernate-mapping>
